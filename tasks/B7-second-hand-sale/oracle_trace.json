{
  "task_id": "B7-2025-GEN",
  "agent_version": "oracle-v1.0.0",
  "steps": [
    {
      "t": 0,
      "act": "open",
      "url": "https://market.local/list-item.html",
      "note": "Open List Item page"
    },
    {
      "t": 1.0,
      "act": "type",
      "selector": "#item-name",
      "value": "Vintage Desk Lamp",
      "note": "Enter item name"
    },
    {
      "t": 2.0,
      "act": "type",
      "selector": "#description",
      "value": "Working condition, minor scratches, includes bulb.",
      "note": "Enter description"
    },
    {
      "t": 3.0,
      "act": "type",
      "selector": "#price",
      "value": "50",
      "note": "Enter price"
    },
    {
      "t": 4.0,
      "act": "select",
      "selector": "#category",
      "value": "home",
      "note": "Select category"
    },
    {
      "t": 5.0,
      "act": "click",
      "selector": "button.pri",
      "note": "Submit listing"
    },
    {
      "t": 6.0,
      "act": "wait",
      "selector": "#items-list",
      "note": "Wait for items list on index page"
    },
    {
      "t": 7.0,
      "act": "extract",
      "selector": "#items-list",
      "variable": "market.listed_items.last.id",
      "note": "Simulate item ID extraction (as ID not visible on page)"
    }
  ]
}
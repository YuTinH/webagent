    },
    {
      "t": 7,
      "act": "assert",
      "selector": "div.text-sm.muted:has-text('Status: enrolled')",
      "note": "Check enrollment status is 'enrolled'"
    },
    {
      "t": 9,
      "act": "extract",
      "variable": "enroll-state",
      "selector": "div.text-sm.muted",
      "note": "Extract enrolled status for assertion"
    }
  ]
}
